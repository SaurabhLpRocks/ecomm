{
  "name": "ecomm-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "install-ui-deps": "npm i --legacy-peer-deps",
    "start": "nx serve --hmr",
    "start:ecomm-ui": "nx run ecomm-ui:serve --ssl true --proxy-config apps/ecomm-ui/src/proxy.conf.json --host=127.0.0.1",
    "build": "nx build",
    "build:prod:ecomm-ui": "cross-env NODE_ENV=production ng build --prod=true",
    "build:staging:ecomm-ui": "cross-env NODE_ENV=staging ng build --configuration=staging",
    "storybook:ecomm-ui": "nx run ecomm-ui:storybook",
    "test": "nx test",
    "lint": "nx workspace-lint && ng lint",
    "eslint:fix": "npx eslint --fix",
    "prettier:fix": "npx prettier --write",
    "node_modules:delete": "rimraf node_modules",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint:diff-develop": "nx affected:lint --parallel --maxParallel=8 --base=origin/develop",
    "affected:lint": "nx affected:lint --parallel --maxParallel=8 --uncommitted --fix=true",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write --parallel --maxParallel=8  --uncommitted -- --base main",
    "format:check": "nx format:check",
    "affected:lint:format": "npm run affected:lint && npm run format:write",
    "update": "nx migrate latest",
    "update:apply": "nx migrate --run-migrations",
    "ng:update": "cross-env FORCE_NG_UPDATE=true ng update @angular/core @angular/cli --force",
    "update:material": "nx migrate  @angular/material@latest  @angular/cdk@latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "bundle:report:ecomm-ui": "ng build --prod --source-map && source-map-explorer dist/apps/ecomm-ui/**/*.js",
    "build:ecomm-ui:debug": "ng build --source-map --optimization=false --aot=false --build-optimizer=false",
    "build:ecomm-ui:debug:staging": "cross-env NODE_ENV=staging ng build --configuration=staging --source-map"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.0.1",
    "@angular/cdk": "15.0.0",
    "@angular/common": "15.0.1",
    "@angular/compiler": "15.0.1",
    "@angular/core": "15.0.1",
    "@angular/flex-layout": "~14.0.0-beta.41",
    "@angular/forms": "15.0.1",
    "@angular/material": "15.0.0",
    "@angular/platform-browser": "15.0.1",
    "@angular/platform-browser-dynamic": "15.0.1",
    "@angular/router": "15.0.1",
    "@fortawesome/angular-fontawesome": "^0.12.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@ngneat/dirty-check-forms": "^3.0.3",
    "@ngneat/error-tailor": "^3.0.2",
    "@ngneat/hot-toast": "^4.1.0",
    "@ngneat/subscribe": "^4.0.0",
    "@nrwl/angular": "15.3.3",
    "date-fns": "^2.29.3",
    "lodash-es": "^4.17.21",
    "ngx-cookie-service": "^15.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.1",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "15.0.1",
    "@angular/language-service": "15.0.1",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nrwl/cli": "15.3.3",
    "@nrwl/cypress": "15.3.3",
    "@nrwl/eslint-plugin-nx": "15.3.3",
    "@nrwl/jest": "15.3.3",
    "@nrwl/linter": "15.3.3",
    "@nrwl/nx-cloud": "15.0.2",
    "@nrwl/workspace": "15.3.3",
    "@schematics/angular": "15.0.1",
    "@types/jest": "28.1.8",
    "@types/lodash-es": "4.17.6",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "angular2-uuid": "^1.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^9.1.0",
    "dotenv": "^10.0.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-class-members": "^1.14.1",
    "husky": "^7.0.4",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "12.2.3",
    "lint-staged": "^12.2.0",
    "nx": "15.3.3",
    "prettier": "2.6.2",
    "rimraf": "^3.0.2",
    "source-map-explorer": "^2.5.2",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}

